<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.juwuheback.mapper.GalleryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.juwuheback.domain.entity.Gallery">
        <id column="gallery_id" property="galleryId"/>
        <result column="gallery_path" property="galleryPath"/>
        <result column="gallery_description" property="galleryDescription"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        gallery_id
        , gallery_path, gallery_description
    </sql>

</mapper>
